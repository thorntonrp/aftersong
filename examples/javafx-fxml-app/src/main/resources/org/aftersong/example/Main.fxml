<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.lang.* ?>
<?import java.util.* ?>
<?import javafx.geometry.*?>
<?import javafx.scene.* ?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.* ?>
<?import javafx.scene.image.* ?>
<?import javafx.scene.image.* ?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.* ?>

<?scenebuilder-classpath-element ../../../../../../target/classes?>

<AnchorPane id="AnchorPane" fx:id="rootPane"
			prefHeight="868.0" prefWidth="1290.0"
			style="-fx-background-color: #A0A0C0"
			xmlns:fx="http://javafx.com/fxml"
			fx:controller="org.aftersong.example.MainController">
  <children>
    <VBox fx:id="leftPane" fillWidth="true" onMouseEntered="#show"
		  onMouseExited="#hide" style="-fx-background-color: #EFEFEFFF;"
		  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
		  AnchorPane.topAnchor="0.0">
      <ImageView>
        <image>
          <Image url="org/aftersong/example/icon-256.png" />
        </image>
        <VBox.margin>
          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
        </VBox.margin>
      </ImageView>
    </VBox>
    <VBox fx:id="rightPane" fillWidth="true" onMouseEntered="#show"
		  onMouseExited="#hide" style="-fx-background-color: #EFEFEFFF;"
		  AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0"
		  AnchorPane.topAnchor="0.0">
      <ImageView VBox.margin="$x1">
        <image>
          <Image url="org/aftersong/example/icon-256.png" />
        </image>
      </ImageView>
    </VBox>
    <HBox fx:id="topPane" fillHeight="true" onMouseEntered="#show"
		  onMouseExited="#hide" style="-fx-background-color: #EFEFEFFF;"
		  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
		  AnchorPane.topAnchor="0.0">
      <ImageView HBox.margin="$x1">
        <image>
          <Image url="org/aftersong/example/icon-256.png" />
        </image>
      </ImageView>
    </HBox>
    <HBox fx:id="bottomPane" fillHeight="true" onMouseEntered="#show"
		  onMouseExited="#hide" style="-fx-background-color: #EFEFEFFF;"
		  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
		  AnchorPane.rightAnchor="0.0">
      <ImageView HBox.margin="$x1">
        <image>
          <Image url="org/aftersong/example/icon-256.png" />
        </image>
      </ImageView>
    </HBox>
  </children>
</AnchorPane>
